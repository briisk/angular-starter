{
  "name": "angular-starter",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "author": "Daniel Koza <daniel.koza@briisk.co> (https://github.com/dakolech)",
  "scripts": {
    "precommit": "npm run lint",
    "prepush": "ng test --single-run",
    "init": "./copyEnvFiles.sh",
    "ng": "ng",
    "start": "ng serve",
    "start:production": "ng serve --env=prod --aot --target=production",
    "build:production": "ng build --env=prod --aot --target=production",
    "build:staging": "ng build --env=staging --aot --target=production",
    "build:workspace": "ng build --env=workspace --aot --target=production",
    "build:dev": "ng build",
    "test": "ng test",
    "test:ci": "ng test --single-run",
    "lint": "npm run lint:scss && npm run lint:ts && npm run lint:html",
    "lint:ts": "tslint \"src/**/*.ts\"",
    "lint:scss": "sass-lint src/",
    "lint:html": "htmllint -- 'src/**/*.html'",
    "e2e": "ng e2e"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@briisk/http-wrapper": "^0.4.1",
    "@ngrx/effects": "^4.0.5",
    "@ngrx/router-store": "^4.0.4",
    "@ngrx/store": "^4.0.3",
    "@ngrx/store-devtools": "^4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.1",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@briisk/tslint-config": "^0.0.2",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "htmllint": "^0.6.0",
    "htmllint-cli": "0.0.6",
    "husky": "^0.14.3",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "sass-lint": "^1.10.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
