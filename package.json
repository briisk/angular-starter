{
  "name": "angular-starter",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "private": false,
  "author": "Daniel Koza <daniel.koza@briisk.co> (https://github.com/dakolech)",
  "scripts": {
    "precommit": "npm run lint",
    "prepush": "ng test --single-run",
    "init": "./copyEnvFiles.sh",
    "ng": "ng",
    "start": "ng serve",
    "start:production": "ng serve --env=prod --aot --target=production",
    "build:production": "ng build --env=prod --aot --target=production",
    "build:staging": "ng build --env=staging --aot --target=production",
    "build:workspace": "ng build --env=workspace --aot --target=production",
    "build:dev": "ng build",
    "test": "ng test",
    "test:ci": "ng test --single-run",
    "lint": "npm run lint:scss && npm run lint:ts && npm run lint:html",
    "lint:ts": "tslint \"src/**/*.ts\"",
    "lint:scss": "sass-lint -c .sass-lint.yml -v",
    "lint:html": "htmllint -- 'src/**/*.html'",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@ngrx/effects": "^6.0.0-beta.3",
    "@ngrx/router-store": "^6.0.0-beta.3",
    "@ngrx/store": "^6.0.0-beta.3",
    "@ngrx/store-devtools": "^6.0.0-beta.3",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "core-js": "^2.5.6",
    "ngrx-store-localstorage": "^5.0.0",
    "ramda": "^0.25.0",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.26",
    "@angular/cdk": "^6.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular/cli": "^6.0.1",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@briisk/tslint-config": "^0.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.0.8",
    "codelyzer": "~4.3.0",
    "htmllint": "^0.7.2",
    "htmllint-cli": "0.0.7",
    "husky": "^0.14.3",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.1.0",
    "node-sass": "^4.9.0",
    "protractor": "~5.3.2",
    "sass-lint": "^1.12.1",
    "ts-node": "~6.0.3",
    "tslint": "~5.10.0",
    "typescript": "~2.7.2"
  }
}
